    "生成斐波那契数列\n",
    "Author:Ender\n",
    "Version:0.1\n",
    "\"\"\"\n",
    "num=int(input('输入层数：'))\n",
    "def fb(n):\n",
    "    if n<=2:\n",
    "        return 1\n",
    "    else:\n",
    "        return fb(n-1)+fb(n-2)\n",
    "print(fb(num))\n",
